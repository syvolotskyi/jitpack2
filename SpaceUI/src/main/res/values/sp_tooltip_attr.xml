<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SPTooltipView">
        <attr name="android:text" format="string|reference" />
        <attr name="android:textAppearance" />
        <attr name="backgroundBoxColor" format="reference"/>
        <attr name="directionArrow" format="enum">
            <enum name="none" value="0" />
            <enum name="top_center" value="1" />
            <enum name="top_left" value="2" />
            <enum name="top_right" value="3" />
            <enum name="bottom_center" value="4" />
            <enum name="bottom_left" value="5" />
            <enum name="bottom_right" value="6" />
            <enum name="left" value="7" />
            <enum name="right" value="8" />
        </attr>
    </declare-styleable>
</resources>